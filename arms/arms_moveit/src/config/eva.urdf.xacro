<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="eva">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import eva urdf file -->
    <xacro:include filename="$(find eva)/urdf/eva.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="eva.ros2_control.xacro" />


    <xacro:eva_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
